{
  "name": "@bandwidth/shared-components",
  "version": "2.0.0",
  "description": "Shared Component Library for Bandwidth React Apps",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --config jestConfig.json --coverage",
    "watch:run": "babel --watch --copy-files --out-dir dist src",
    "watch": "run-s watch:*",
    "build:clean": "rimraf dist",
    "build:cleanDocs": "rimraf docs",
    "build:index": "node ./tools/generateIndices.js",
    "build:dist": "babel --copy-files --out-dir dist src",
    "build:styleguide": "styleguidist build",
    "build": "run-s build:*",
    "prepublish": "npm run build",
    "styleguide": "styleguidist server",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma all --write \"{src,__tests__}/**/*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bandwidth/shared-components.git"
  },
  "author": "Grant <gaforres@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bandwidth/shared-components/issues"
  },
  "homepage": "https://github.com/Bandwidth/shared-components#readme",
  "dependencies": {
    "animation-timer": "^1.0.12",
    "change-case": "^3.0.1",
    "functional-easing": "^1.0.8",
    "prop-types": "^15.5.8",
    "provinces": "^1.11.0",
    "react": "^15.5.4",
    "react-collapse": "^4.0.3",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.13.1",
    "react-motion": "^0.5.1",
    "react-router-dom": "^4.1.1",
    "react-syntax-highlighter": "^5.5.0",
    "styled-components": "^2.1.2",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "file": "^0.2.2",
    "file-loader": "^0.11.2",
    "google-fonts-webpack-plugin": "^0.4.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.4.0",
    "jest-styled-components": "^3.1.5",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^4.0.2",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "react-docgen": "^2.17.0",
    "prettier": "^1.5.3",
    "react-dom": "^15.6.1",
    "react-styleguidist": "^6.0.21",
    "react-test-renderer": "^15.6.1",
    "react-x-ray": "^1.0.0-2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "verbal-expressions": "^0.3.0",
    "webpack": "^3.5.5"
  }
}
