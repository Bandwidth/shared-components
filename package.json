{
  "name": "@bandwidth/shared-components",
  "version": "3.2.6",
  "description": "Shared Component Library for Bandwidth React Apps",
  "main": "dist/index.js",
  "scripts": {
    "test": "cross-env NODE_PATH=src jest --config jestConfig.json --coverage",
    "watch:run": "npm run build:index && cross-env NODE_PATH=src babel --watch --copy-files --out-dir dist src",
    "watch": "run-s watch:*",
    "build:clean": "rimraf dist",
    "build:dist": "cross-env NODE_PATH=src babel --copy-files --out-dir dist src",
    "build:index": "node ./tools/generateIndices.js",
    "build:styleguide": "cross-env NODE_PATH=src styleguidist build",
    "build": "run-s build:*",
    "prepublish": "npm run build",
    "styleguide:prerun": "npm run build:index",
    "styleguide:run": "cross-env NODE_PATH=src styleguidist server",
    "styleguide": "run-s styleguide:*",
    "gen": "studs generate component",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --trailing-comma all --write \"{src,tools,__tests__}/**/*.js\"",
    "start": "npm run styleguide"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bandwidth/shared-components.git"
  },
  "author": "Grant <gaforres@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bandwidth/shared-components/issues"
  },
  "homepage": "https://github.com/Bandwidth/shared-components#readme",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^3.2.3",
    "prop-types": "^15.5.8"
  },
  "dependencies": {
    "animation-timer": "^1.0.12",
    "change-case": "^3.0.1",
    "dedent": "^0.7.0",
    "functional-easing": "^1.0.8",
    "provinces": "^1.11.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-autosuggest": "^9.3.2",
    "react-collapse": "^4.0.3",
    "react-dates": "^13.0.6",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dropzone": "^3.13.1",
    "react-motion": "^0.5.1",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.10",
    "react-syntax-highlighter": "^5.5.0",
    "recompose": "^0.26.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-styled-components": "https://registry.npmjs.org/babel-plugin-styled-components/-/babel-plugin-styled-components-1.2.0.tgz",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "file": "^0.2.2",
    "file-loader": "^0.11.2",
    "google-fonts-webpack-plugin": "^0.4.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.4.0",
    "jest-styled-components": "^3.1.5",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^5.0.0",
    "lodash": "^4.17.4",
    "lorem-ipsum": "^1.0.4",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.8.2",
    "react": "^16.0.0",
    "react-docgen": "^3.0.0-beta9",
    "react-docgen-annotation-resolver": "^1.0.0",
    "react-dom": "^16.0.0",
    "react-styleguidist": "^6.5.1",
    "react-test-renderer": "^15.6.1",
    "react-x-ray": "^1.0.0-2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "styled-components": "^3.2.3",
    "verbal-expressions": "^0.3.0",
    "webpack": "^3.5.5"
  }
}
