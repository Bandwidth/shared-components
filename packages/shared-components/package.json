{
  "name": "@bandwidth/shared-components",
  "version": "5.0.8",
  "description": "Shared Component Library for Bandwidth React Apps",
  "main": "dist/index.js",
  "scripts": {
    "test": "cross-env NODE_PATH=src jest --config jestConfig.json --coverage",
    "watch:run": "npm run build:index && cross-env NODE_PATH=src babel --watch --copy-files --out-dir dist src",
    "watch": "run-s watch:*",
    "build:clean": "rimraf dist",
    "build:dist": "cross-env NODE_PATH=src babel --copy-files --out-dir dist src",
    "build:index": "node ./tools/generateIndices.js",
    "build:docs": "cross-env NODE_PATH=src styleguidist build",
    "build": "run-s build:*",
    "prepublish": "npm run build",
    "styleguide:prerun": "npm run build:index",
    "styleguide:run": "cross-env NODE_PATH=src styleguidist server",
    "styleguide": "run-s styleguide:*",
    "start": "npm run styleguide",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
