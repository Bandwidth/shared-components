{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions"]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "babel-plugin-transform-class-properties",
    ["inline-react-svg", { "ignorePattern": "fonts/*" }],
    "styled-components",
    [
      "module-resolver",
      {
        "root": ["./src"]
      }
    ]
  ]
}
